programa {
  funcao inicio() {
    inteiro numero

    escreva("Digite um numero: ")
    leia(numero)

    se(numero % 5 == 0){
      escreva("é multiplo por 5")
    }
    senao{
      escreva("não é multiplo por 5")
    }
  }
}

